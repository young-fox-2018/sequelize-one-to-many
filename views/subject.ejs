<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>Subjects data</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body>
    <center>
        <div class="header">
            <h1>All Subjects Data</h1>
        </div>
    </center>
    <div class="table">
        <table>
            <thead>
                <th>id</th>
                <th>Subject Name</th>
                <th>Teacher</th>
            </thead>
            <tbody>
                <% data.forEach(subject => { %>
                <tr>
                    <td>
                        <%= subject.id %>
                    </td>
                    <td>
                        <%= subject.subjectName %>
                    </td>
                    <td>
                        <ol>
                        <% subject.Teachers.forEach(teacher => { %>
                            <li>
                            <%= `${teacher.firstName} ${teacher.lastName}` %>
                            </li>
                        <% }) %>
                        </ol>
                    </td>
                </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
</body>
</html>